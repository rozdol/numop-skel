{
    "name": "rozdol/numop-skel",
    "description": "Test skeleton app",
    "type": "project",
    "license": "MIT",
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:rozdol/numop-assets.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:rozdol/numop.git"
        }
    ],
    "require": {
        "php": ">=5.6",
        "rozdol/numop": "1.*",
        "rozdol/numop-assets": "1.*",
        "composer/installers": "^1.1"
    },
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "extra":
    {
        "installer-types" : [ "library" , "bower-asset", "vanilla-theme"],
        "installer-paths":
        {
            "public/{$name}": ["type:vanilla-theme"]
        }
    }
}
