{
    "name": "rozdol/numop-skel",
    "description": "Test skeleton app",
    "type": "project",
    "license": "MIT",
    "repositories": [

        {
            "type": "vcs",
            "url": "git@github.com:rozdol/numop.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:rozdol/numop-assets.git"
        }
    ],
    "require": {
        "php": ">=5.6",
        "rozdol/numop": "1.*",
        "rozdol/numop-assets": "1.*",
        "rozdol/numop-src": "1.*"
    },
    "extra":
    {
        "installer-paths":
        {
            "public/{$name}": ["rozdol/numop-assets"],
            "{$name}": ["rozdol/numop-src"]
        }
    }
}
